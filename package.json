{
  "name": "@novasign/json-schema-editor",
  "version": "0.3.0",
  "private": false,
  "description": "Json Schema 可视化编辑器",
  "keywords": [
    "json",
    "schema",
    "json schema",
    "json schema editor"
  ],
  "homepage": "https://lin-mt.github.io/json-schema-editor-antd",
  "repository": {
    "type": "git",
    "url": "https://github.com/lin-mt/json-schema-editor-antd.git"
  },
  "license": "MIT",
  "exports": {
    ".": "./src/index.ts"
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build-scoped": "father build",
    "build:watch": "father dev",
    "dev-scoped": "dumi dev",
    "docs:build": "dumi build",
    "doctor": "father doctor",
    "lint:css": "stylelint \"{src,test}/**/*.{css,less}\"",
    "lint:es": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "prepare-scoped": "husky install && dumi setup",
    "prepublishOnly": "father doctor && npm run build",
    "start-scoped": "npm run dev"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "@ant-design/icons": "5.3.6",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@monaco-editor/react": "4.6.0",
    "@types/lodash": "4.14.186",
    "@types/react": "18.3.2",
    "@types/react-dom": "18.3.0",
    "@umijs/lint": "^4.0.0",
    "antd": "^5.2.3",
    "dumi": "^2.0.2",
    "eslint": "^8.23.0",
    "father": "^4.1.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "lodash": "4.17.21",
    "mobx": "^6.8.0",
    "mobx-react": "^7.6.0",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prettier-plugin-packagejson": "^2.2.18",
    "rc-util": "5.39.1",
    "react": "18.3.1",
    "react-dom": "18.2.0",
    "stylelint": "^14.9.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "authors": [
    "lin-mt <lin-mt@outlook.com>"
  ]
}
